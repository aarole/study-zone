<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aarole.study_zone">

    <uses-permission android:name="android.permission.READ_CALENDAR" />
    <uses-permission android:name="android.permission.WRITE_CALENDAR" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/sz_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/sz_logo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version"/>
        <activity android:name=".splashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity"/>
        <activity android:name=".pickActivity" />
        <activity android:name=".remList" />
        <activity android:name=".endDate" />
        <activity android:name=".startTime" />
        <activity android:name=".MainActivity" />
        <activity android:name=".playlistPicker" />
        <activity android:name=".todo" />
        <activity android:name=".stats" />
        <activity android:name=".addCourse" />
    </application>

</manifest>